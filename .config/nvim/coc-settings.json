{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  },
  "diagnostic.displayByAle": true,
  "python.jediEnabled": false,
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.timeout": 1000
}


